<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Register">
   <children>
      <TextField fx:id="nameText" layoutX="199.0" layoutY="134.0" promptText="real name" />
      <PasswordField fx:id="passwdText" layoutX="199.0" layoutY="185.0" promptText="passwd" />
      <PasswordField fx:id="passwdCheckText" layoutX="199.0" layoutY="232.0" promptText="passwd, again" />
      <TextField fx:id="idText" layoutX="199.0" layoutY="87.0" promptText="id" />
      <Button fx:id="registerUserButton" layoutX="199.0" layoutY="289.0" mnemonicParsing="false" onAction="#registerHook" text="Register" />
      <Button fx:id="cancelButton" layoutX="332.0" layoutY="289.0" mnemonicParsing="false" onAction="#cancelHook" text="Cancel" />
   </children>
</AnchorPane>
